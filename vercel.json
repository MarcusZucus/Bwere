{
  "routes": [
    {
      "src": "/service-worker.js",
      "dest": "/service-worker.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/html_modules/mis_compras/mis_compras.html",
      "dest": "/html_modules/mis_compras/mis_compras.html"
    },
    {
      "src": "/html_modules/home/home.html",
      "dest": "/html_modules/home/home.html"
    },
    {
      "src": "/html_modules/modules/menu.js",
      "dest": "/html_modules/modules/menu.js"
    },
    {
      "src": "/html_modules/media-queries.css",
      "dest": "/html_modules/media-queries.css"
    },
    {
      "src": "/html_modules/styles.css",
      "dest": "/html_modules/styles.css"
    },
    {
      "src": "/html_modules/menu.css",
      "dest": "/html_modules/menu.css"
    },
    {
      "src": "/html_modules/input-buttons.css",
      "dest": "/html_modules/input-buttons.css"
    },
    {
      "src": "/icon-192x192.png",
      "dest": "/icon-192x192.png"
    },
    {
      "src": "/icon-512x512.png",
      "dest": "/icon-512x512.png"
    },
    {
      "src": "/fonts.googleapis.com/(.*)",
      "dest": "https://fonts.googleapis.com/$1"
    },
    {
      "src": "/fonts.gstatic.com/(.*)",
      "dest": "https://fonts.gstatic.com/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=0, s-maxage=31536000"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline'; connect-src 'self' https://fonts.googleapis.com https://fonts.gstatic.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data:;"
        }
      ]
    }
  ]
}
